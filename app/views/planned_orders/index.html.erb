<h1>Listing planned_orders</h1>

<table>
  <tr>
    <th>Articlecode</th>
    <th>Supplier</th>
    <th>Quantity</th>
    <th>Creationdate</th>
    <th>Expecteddeliverydate</th>
    <th>Validated</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @planned_orders.each do |planned_order| %>
  <tr>
    <td><%= planned_order.articleCode %></td>
    <td><%= planned_order.supplier %></td>
    <td><%= planned_order.quantity %></td>
    <td><%= planned_order.creationDate %></td>
    <td><%= planned_order.expectedDeliveryDate %></td>
    <td><%= planned_order.validated %></td>
    <td><%= link_to 'Show', planned_order %></td>
    <td><%= link_to 'Edit', edit_planned_order_path(planned_order) %></td>
    <td><%= link_to 'Destroy', planned_order, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Planned order', new_planned_order_path %>
